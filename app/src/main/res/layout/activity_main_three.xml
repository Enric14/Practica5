<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.google.android.material.appbar.MaterialToolbar
        android:id="@+id/main3_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="?attr/colorPrimary"
        app:menu="@menu/main_activity_tres_menu"
        app:titleTextColor="@color/white"
        app:title="@string/main3_title_toolbar"
        app:navigationIcon="@drawable/baseline_density_medium_24"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.google.android.material.button.MaterialButton
        android:id="@+id/main3_btn1"
        style="?appPractica5BtnFilledpr"
        android:layout_marginVertical="60dp"
        android:layout_marginStart="40dp"
        app:icon="@drawable/baseline_camera_alt_24"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/main3_btn2"
        style="?appPractica5BtnFilledse"
        android:layout_marginVertical="60dp"
        app:icon="@drawable/baseline_favorite_24"
        app:layout_constraintEnd_toStartOf="@+id/main3_btn3"
        app:layout_constraintStart_toEndOf="@+id/main3_btn1"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/main3_btn3"
        style="?appPractica5BtnFilledth"
        android:layout_marginVertical="60dp"
        android:layout_marginEnd="25dp"
        app:icon="@drawable/baseline_landscape_24"
        app:layout_constraintEnd_toStartOf="@+id/main3_btn4"
        app:layout_constraintTop_toTopOf="parent" />

    <com.google.android.material.button.MaterialButton
        android:id="@+id/main3_btn4"
        style="?appPractica5BtnFilledfo"
        android:layout_marginVertical="60dp"
        android:layout_marginEnd="40dp"
        app:icon="@drawable/baseline_face_24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.cardview.widget.CardView
        android:id="@+id/main3_card1_app"
        android:layout_width="match_parent"
        android:layout_height="270dp"
        android:layout_marginHorizontal="50dp"
        android:layout_marginVertical="150dp"
        app:layout_constraintTop_toTopOf="parent"
        app:cardCornerRadius="40dp"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main3_img_card1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="centerCrop"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/img_calendario" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/main3_text1_app"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginVertical="10dp"
                android:layout_marginStart="20dp"
                android:text="@string/main2_description_card1"
                android:textAppearance="?appPractica5H5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/main2_text2_app"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/main2_desription2_card1"
                android:layout_marginStart="20dp"
                android:layout_marginVertical="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/main3_img_card1"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <androidx.cardview.widget.CardView
        android:id="@+id/main3_card2_app"
        android:layout_width="match_parent"
        android:layout_height="270dp"
        android:layout_marginHorizontal="50dp"
        android:layout_marginVertical="450dp"
        app:layout_constraintTop_toTopOf="parent"
        app:cardCornerRadius="40dp"
        >

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/main3_img_card2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:scaleType="centerCrop"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/img_camping" />


            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/main3_text3_app"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginVertical="10dp"
                android:layout_marginStart="20dp"
                android:text="@string/main_description_card2"
                android:textAppearance="?appPractica5H5"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                />

            <com.google.android.material.textview.MaterialTextView
                android:id="@+id/main3_text4_app"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/main_description2_card2"
                android:layout_marginStart="20dp"
                android:layout_marginVertical="15dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/main3_img_card2"
                />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <FrameLayout
        android:id="@+id/main3_content_fragment"
        android:layout_width="match_parent"
        android:layout_height="1200dp"
        app:layout_constraintTop_toBottomOf="@+id/main3_toolbar" />

</androidx.constraintlayout.widget.ConstraintLayout>